{
    "cli": {
        "version": ">= 5.2.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "http://192.168.1.18:3001/api"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://fitzo.onrender.com/api",
                "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "1030039443378-ek0r7ar68qfjga2dmj5inr7e715g5c1f.apps.googleusercontent.com",
                "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "1030039443378-u3vdk2fq3ebu90sv2okvimbta1rs2d7v.apps.googleusercontent.com"
            }
        },
        "production": {
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "resourceClass": "m-medium"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://fitzo.onrender.com/api",
                "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "1030039443378-ek0r7ar68qfjga2dmj5inr7e715g5c1f.apps.googleusercontent.com",
                "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "1030039443378-u3vdk2fq3ebu90sv2okvimbta1rs2d7v.apps.googleusercontent.com"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "./google-services-key.json",
                "track": "production"
            },
            "ios": {
                "appleId": "your-apple-id@email.com",
                "ascAppId": "your-app-store-connect-app-id"
            }
        }
    }
}