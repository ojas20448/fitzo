# Production environment variables for Fitzo Mobile
# Copy this to .env and replace with your production values

# Backend API URL (your Render deployment)
EXPO_PUBLIC_API_URL=https://fitzo-backend.onrender.com/api

# Google OAuth Client IDs (get from Google Cloud Console)
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your-android-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-web-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com
